{
	"private": true,
	"name": "turbo-nextui-starter",
	"workspaces": [
		"apps/*",
		"packages/*",
		"server/*"
	],
	"scripts": {
		"build": "turbo build --no-daemon",
		"dev": "turbo dev --no-daemon --parallel",
		"start": "turbo start --no-daemon --parallel",
		"lint": "turbo lint --no-daemon --parallel",
    "prepare": "husky install"
	},
	"devDependencies": {
		"eslint": "8.48.0",
		"eslint-config-custom": "*",
		"husky": "^9.1.4",
		"prettier": "3.3.3",
		"turbo": "2.0.14"
	},
	"packageManager": "yarn@1.22.22"
}
